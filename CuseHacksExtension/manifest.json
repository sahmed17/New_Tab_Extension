{
  "name": "CuseHacks New Tab",
  "version": "0.2.1.0",
  "description": "Displays date, time, weather, and diversity fact of the day!",
  "manifest_version": 2,
  "author": "Sayf Ahmed & Carolina",
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },
"browser_action": {
   "default_icon": { "16": "icon16.png" },
   "default_title": "CuseHacks New Tab"
 },
"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
"permissions": ["activeTab", "geolocation"]
}

